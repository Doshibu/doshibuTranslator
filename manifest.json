{
    "name": "Doshibu Translator",
    "version" : "1",
    "manifest_version": 2,
    "description": "Doshibu Translator",
    "icons": {
        "48": "skin/icon/icon-48.png",
        "128": "skin/icon/icon-128.png"
    },
    "browser_action": {
        "name": "Doshibu Translator",
        "default_title": "",
        "default_icon": {
            "19": "skin/icon/icon-19.png"
        },
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "cookies",
        "privacy",
        "tabs",
        "pageCapture",
        "http://*/*",
        "https://*/*",
        "nativeMessaging",
        "management",
        "<all_urls>"
    ],
    "homepage_url": "https://github.com/Doshibu/doshibuTranslator"
}
